class IncOperators3 
{
	public static void main(String[] args) 
	{
		int a=2, b=3;
		a++;
		++b;
		int c=a+b;
		System.out.println(a);
		System.out.println(b); 
		System.out.println(c);
		System.out.println(++a);
		System.out.println(b++);
		System.out.println(--c);
	//	c=a++ + ++b;
		//System.out.println(a);
//		System.out.println(b);
	//	System.out.println(c);
	}
}
