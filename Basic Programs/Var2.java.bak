class Var2 
{
	public static void main(String[] args) 
	{
		int id=1234;
		char c='a';
		System.out.println("Id is "+id);
		System.out.println("C is "+c);
		id=2345;//re-assigning
		c=$;
		System.out.println("Id is "+id);
		System.out.println("C is "+c);
		id=4567;//re-assigning
		System.out.println("Id is "+id);
	}
}
